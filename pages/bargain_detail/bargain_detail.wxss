.header {
  background: #ff9939;
  background: -webkit-linear-gradient(top, #ff9939, #fdb75f); /* For Safari 5.1 to 6.0 */
  background: -o-linear-gradient(top, #ff9939, #fdb75f); /* For Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(top, #ff9939, #fdb75f); /* For Firefox 3.6 to 15 */
  background: linear-gradient(top, #ff9939, #fdb75f); /* Standard syntax */
  text-align: center;
  padding: 30rpx 3%;
  position: relative;
}

.header .headurl image {
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
}

.header .name {
  color: #fff;
  font-size: 30rpx;
  margin-top: 10rpx;
}

.header .des {
  color: #fff;
  font-size: 28rpx;
  line-height: 50rpx;
}

.header .arrow {
  position: absolute;
  bottom: -30rpx;
  left: 50%;
  margin-left: -18rpx;
}

.header .arrow image {
  height: 25rpx;
  width: 36rpx;
}

.header .rule {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  width: 116rpx;
  color: #fff;
  border: 2rpx solid #fff;
  border-radius: 8rpx;
  font-size: 24rpx;
}

.class_detail {
  padding: 20rpx 3% 10rpx;
  color: #666;
  background: #fff;
  margin-top: 20rpx;
}

.class_detail .left {
  width: 32%;
  float: left;
}

.class_detail .right {
  width: 65%;
  float: right;
  color: #666;
}

.class_detail .left image {
  width: 100%;
  height: 120rpx;
}

.class_detail .right .course_name {
  height: 50rpx;
  line-height: 50rpx;
  font-size: 30rpx;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-bottom: 15rpx;
  text-align: left;
}

.class_detail .right .other {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  width: 100%;
}

.class_detail .other .price {
  float: left;
}

.class_detail .distance {
  float: right;
}

.class_detail .distance image {
  width: 20rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.class_detail .distance text {
  vertical-align: middle;
  font-size: 24rpx;
  color: #999;
}

.class_detail .other .title {
  font-size: 24rpx;
}

.class_detail .other .active_price {
  color: #F10804;
  font-size: 28rpx;
  margin: 0 10rpx;
}

.class_detail .other .init_price {
  color: #666;
  font-size: 24rpx;
}
.class_detail .other .init_price text{
  color: #F10804;
}

.banner {
  background: #fff;
  width: 100%;
}

.banner image {
  width: 100%;
  height: 360rpx;
}

.box {
  padding: 3%;
  margin-top: 20rpx;
  color: #666;
  text-align: center;
}

.box .rest {
  font-size: 26rpx;
}

.box .rest text {
  font-size: 28rpx;
  color: #f10804;
}

.box .process {
  padding: 20rpx 0;
  font-size: 24rpx;
}

.box .process .left, .box .process .middle {
  float: left;
}

.box .process .left {
  text-align: left;
  width: 12%;
  color: #999;
}

.box .process .right {
  float: right;
  text-align: left;
  width: 12%;
  color: #999;
}

.box .process .middle {
  width: 68%;
  margin: 0 4%;
  height: 20rpx;
  background: #eee;
  border-radius: 10rpx;
  position: relative;
  overflow: hidden;
  margin-top: 22rpx;
}

.box .process .middle .pro_line {
  position: absolute;
  top: 0;
  left: 0;
  height: 20rpx;
  width: 0;
  border-radius: 10rpx;
  background: #f8b184;
  background: -webkit-linear-gradient(left, #f8b184, #ec5056); /* For Safari 5.1 to 6.0 */
  background: -o-linear-gradient(left, #f8b184, #ec5056); /* For Opera 11.1 to 12.0 */
  background: -moz-linear-gradient(left, #f8b184, #ec5056); /* For Firefox 3.6 to 15 */
  background: linear-gradient(left, #f8b184, #ec5056); /* Standard syntax */
}
.box .btn{
  width: 60%;
  margin: 10rpx auto 20rpx;
  height: 70rpx;
  line-height: 70rpx;
  position: relative;

}
.box .btn image{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 70rpx;
}
.box .btn button{
  height: 70rpx;
  line-height: 70rpx;
  color:#fff;
  border-radius: 40rpx;
  font-size: 32rpx;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99999;
  background: transparent;
}
.box .time{
  color: #999;
}
.box .time text{
  color:#F21C19;
}
 
.box .bargain_info{
  background: #eee;
  margin-top: 40rpx;
  padding: 5% 2%;
  padding-top: 0;
}
.box .bargain_info .title{
  line-height: 100rpx;
  color: #666;
  font-size: 34rpx;
}
.box .bargain_info .title text{
  display: inline-block;
  width: 50%;
  text-align: center;
}
.box .bargain_info .title .cur{
  color: #333;
}
.box .bargain_info .bargain_item{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ddd;
  text-align: left;
}
.box .bargain_info .bargain_item:last-child{
  border-bottom: none;
}
.box  .bargain_item image{
  height: 70rpx;
  width:70rpx;
  border-radius: 50%;
  vertical-align: middle;
  margin: 0 20rpx;
}
.box  .bargain_item .name{
  font-size: 28rpx;
  color:#333; 
  width: 15%;
  display: inline-block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle
}

.box  .bargain_item .price{
  width: 40%;
  display: inline-block;
  text-align: center;
  font-size: 25rpx;
  vertical-align: middle

}
.box  .bargain_item .time{
  font-size: 24rpx;
  vertical-align: middle
}
.box .bargain_info .friend .price{
  color:#F21C19;
  vertical-align: middle
}


/*砍价规则弹框*/
page{
  height: 100%;
}
.indexFixed{
  position: fixed;
  top:0;
  left:0;
  bottom:0;
  right:0;
}
.mask{
  position: fixed;
  top:0;
  left:0;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.6;
  z-index: 99;
  overflow: hidden;
 
}
.model{
  width: 80%;
  height: 80%;
  text-align: center;
  position: absolute;
  top: 10%;
  left: 10%;
  z-index: 100
  
}
.model .cancel{
  margin-top: 50rpx;
}
.model .cancel image{
    height: 60rpx;
    width: 60rpx;
}
.model .rule{
  height: 90%;
  width: 100%;
  position: relative;
  background: #fff;
  border-radius: 10rpx;
  z-index: 100;
}
.model .rule image{
  position: absolute;
  height: 106%;
  width: 100%;
  left: 0;
  top: -5%;
}
.model .rule .title{
  position: absolute;
  top: -3.5%;
  z-index: 100;
  color:#fff;
  font-weight: bold;
  font-size: 34rpx;
  left: 35%;
  width: 30%;
  text-align: center;
}
.model .rule .con{
  height: 90%;
  padding: 10% 5% 5%;
  text-align: left;
  width: 90%;
  color: #666;
  font-size: 24rpx;
}
.model .rule .con view{
  margin-bottom: 20rpx;
  list-style-type: disc
  
}
.model .rule .con view text{
  display: inline-block;
  vertical-align:text-top;
  
}
.model .rule .con .circle{
  height: 10rpx;
  width: 10rpx;
  border-radius: 50%;
  background: #999;
  margin-right: 3%;
  margin-top: 10rpx;
  
}
.model .rule .con .txt{
  width: 95%;
  
}
